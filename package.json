{
  "name": "node-book",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./server.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "NODE_ENV=test mocha --opts test/integration/mocha.opts test/integration/**/*_spec.js",
    "test:unit": "NODE_ENV=test mocha --opts test/unit/mocha.opts test/unit/**/*_spec.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --fix --ext .js",
    "prettier:list": "prettier --check '**/*.js'",
    "prettier:fix": "prettier --write '**/*.js'",
    "style:fix": "npm run lint:fix & npm run prettier:fix"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-node6": "^11.0.0",
    "chai": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-node": "^9.1.0",
    "mocha": "^3.2.0",
    "prettier": "1.17.1",
    "sinon": "^2.2.0",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.15.2",
    "config": "^1.29.4",
    "express": "^4.14.0",
    "express-acl": "^2.0.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^4.8.0"
  }
}
